<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>LelangKu - Your Best Auction Place</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="stylesheets/style.css" />
    <link
      href="https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css"
      rel="stylesheet"
    />
  </head>
  <body>
    <%- include ../../partials/client/navbar.ejs %>

    <div class="divider-item-toko"></div>

    <div class="card-profile-toko">
      <div class="row justify-content-between">
        <div class="col-12 mt-0">
          <div class="card-horizontal">
            <div class="img-square-wrapper-toko">
              <img src="assets/images/shoe-1.jpg" />
            </div>
            <div class="card-body card-body-toko">
              <p class="card-title fw-bold">Monkey24</p>
              <div class="lokasi-toko">
                <i class="bx bx-map"></i>
                <span>Bandung</span>
              </div>
              <div class="rating">
                <i class="bx bxs-star" style="color: #ffc400"></i>
                <span>4.9/5 Rating & Ulasan</span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="divider">
      <h4 class="fw-bold">Barang yang di lelang</h4>
    </div>

    <div
      class="container d-flex justify-content-start align-items-start flex-wrap"
    >
      <div class="row">
        <div class="card ms-4">
          <span class="percent">Baru</span>
          <div class="card-image">
            <a class="gambar-hover" href="#"
              ><img
                src="assets/images/shoe-1.jpg"
                width="500"
                style="border-radius: 10px"
              />
              <div class="overlay-card-product">
                <div class="text">View detail</div>
              </div>
            </a>
          </div>
          <div class="card-inner">
            <h5 class="mb-0 fw-bold">Redmi Note 10</h5>
            <div class="price">
              <span>Rp.4.500.000</span>
            </div>
            <span>Kab.Bandung</span>

            <div class="detail-item mt-3">
              <span class="">Berakhir pada</span>
            </div>
            <span class="fw-bold">15 Desember 2022</span>
            <div class="detail-item-waktu">
              <span class="fw-bold">05:00</span>
            </div>
          </div>
        </div>

        <div class="card">
          <span class="percent">Baru</span>
          <div class="card-image">
            <a class="gambar-hover" href="#"
              ><img
                src="assets/images/shoe-1.jpg"
                width="500"
                style="border-radius: 10px"
              />
              <div class="overlay-card-product">
                <div class="text">View detail</div>
              </div>
            </a>
          </div>
          <div class="card-inner">
            <h5 class="mb-0 fw-bold">Redmi Note 10</h5>
            <div class="price">
              <span>Rp.4.500.000</span>
            </div>
            <span>Kab.Bandung</span>

            <div class="detail-item mt-3">
              <span class="">Berakhir pada</span>
            </div>
            <span class="fw-bold">15 Desember 2022</span>
            <div class="detail-item-waktu">
              <span class="fw-bold">05:00</span>
            </div>
          </div>
        </div>

    <%- include ../../partials/client/footer.ejs %>

    <script src="javascripts/js.js"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3"
      crossorigin="anonymous"
    ></script>
  </body>
</html>
